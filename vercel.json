
{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/blog-integration/(.*)",
      "dest": "/blog-integration/index.html"
    },
    {
      "src": "/blog",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
